{
  "name": "vue3-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:css": "stylelint \"./**/*.{css,scss,sass,vue,html}\"",
    "lint:css:fix": "stylelint --fix \"./**/*.{css,scss,sass,vue,html}\"",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --ignore-path .gitignore --fix ."
  },
  "dependencies": {
    "element-plus": "^2.4.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@iconify-json/ep": "^1.1.13",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "markdownlint-cli": "^0.37.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-icons": "^0.18.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
